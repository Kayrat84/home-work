pervaya = 0
vtoray = 0
tretay = 0
chetv = 0
q = []
for i in range(100):
    a = input(' ')
    q.append(a.split(' '))
    if a == '':
        break
for i in q:
    if len(i) == 2:
        if int(i[0])>0 and int(i[1])>0:
            pervaya += 1
        elif int(i[0])<0 and int(i[1])>0:
            vtoray += 1
        elif int(i[0])<0 and int(i[1])<0:
            tretay += 1
        elif int(i[0])>0 and int(i[1])<0:
            chetv += 1 
print('Первая четверть', ': ',pervaya)
print('Вторая четверть', ': ',vtoray)
print('Третья четверть', ': ',tretay)
print('Четвертая четверть', ': ',chetv)
